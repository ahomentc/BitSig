project './BitSigApp.xcodeproj'
target 'BitSigApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BitSigApp
#  pod 'web3swift'
  pod 'web3swift', :git => 'https://github.com/ahomentc/web3swift.git', :branch => 'develop'
