  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'
  pod 'GoogleSignIn'
  pod 'SAConfettiView'
  pod 'Kingfisher', '~> 6.0'
  pod 'lottie-ios'
  pod 'SwiftyJSON'
  pod 'NVActivityIndicatorView'
end
